{
	"quoteProps": "consistent",
	"overrides": [
		{
			"files": ["*.html", "*.css"],
			"options": {
				"printWidth": 9999
			}
		},
		{
			"files": ["*.js", "*.jsx", "*.vue"],
			"options": {
				"singleAttributePerLine": true
			}
		},
		{
			"files": ["*.jsonc"],
			"options": {
				"trailingComma": "none"
			}
		}
	]
}
